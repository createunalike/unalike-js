{
  "name": "@createunalike/unalike-js",
  "version": "0.17.6",
  "description": "Unalike Javascript SDK",
  "license": "MIT",
  "keywords": [
    "unalike",
    "content",
    "api"
  ],
  "author": "Unalike <hello@unalike.xyz>",
  "main": "dist/unalike.min.js",
  "module": "dist/unalike.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "lint": "eslint --quiet --ext .js src",
    "test": "npm run lint && mocha test/index.js --timeout 200000",
    "watch": "npm-watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js",
      "ignore": "dist/unalike.min.js",
      "quiet": false,
      "runOnChangeOnly": false
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "^2.0.1",
    "chai": "^4.3.4",
    "domexception": "2.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsdoc": "^35.4.0",
    "file-api": "0.10.4",
    "filereader": "0.10.3",
    "husky": "^6.0.0",
    "jsondiffpatch": "0.4.1",
    "mocha": "^9.0.1",
    "npm-watch": "^0.10.0",
    "promisepipe": "3.0.0",
    "quill-delta-to-html": "0.12.0",
    "regenerator-runtime": "0.13.7",
    "request": "2.88.2",
    "rollup": "^2.48.0",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "7.0.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "0.21.1",
    "bottleneck": "2.19.5"
  }
}
